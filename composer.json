{
    "name": "designbycode/google-maps-place-details",
    "description": "The Reviews class is a part of the Designbycode namespace and is used to fetch reviews, rating, and user ratings total of a place using the Google Places API. It requires a Google Maps API key and a Google Maps place ID to make requests.",
    "keywords": [
        "designbycode",
        "reviews",
        "google reviews",
        "google maps",
        "google maps reviews"
    ],
    "homepage": "https://github.com/designbycode/google-maps-place-details",
    "license": "MIT",
    "authors": [
        {
            "name": "claudemyburgh",
            "email": "claude@designbycode.co.za",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1 | ^8.2 | ^8.3",
        "guzzlehttp/guzzle": "^7.8"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "mockery/mockery": "^1.6",
        "pestphp/pest": "^2.20",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Designbycode\\GoogleMapsPlaceDetails\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Designbycode\\GoogleMapsPlaceDetails\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
